{
  "name": "daenen-react",
  "version": "1.0.0",
  "description": "Homepage for www.daenen4.de",
  "main": "server.js",
  "repository": "https://github.com/luhmann/daenen-react.git",
  "scripts": {
    "start-react": "BABEL_ENV=development webpack-dev-server",
    "watch-react": "webpack --watch",
    "build-react": "webpack",
    "watch-server": "webpack --config webpack.config.server.js --watch",
    "build-server": "webpack --config webpack.config.server.js",
    "nodemon-server": "nodemon ./dist/server.js",
    "start": "NODE_ENV=DEBUG concurrent --kill-others \"npm run watch-server\" \"npm run nodemon-server\" \"npm run watch-react\"",
    "build": "NODE_ENV=production npm run test && npm run build-react && npm run build-server",
    "test": "BABEL_ENV=test mocha --require ./test/setup.js --compilers js:babel-core/register --recursive",
    "test-debug": "BABEL_ENV=test mocha --debug-brk --require ./test/setup.js --compilers js:babel-core/register --recursive",
    "test-e2e": "nightwatch -e chrome",
    "postinstall": "if [ -z \"$npm_config_production\" ]; then selenium-standalone install; fi",
    "lint": "jscs .",
    "deploy": "fly production"
  },
  "author": "Jan Florian Dietrich",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.5",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.4",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.8",
    "concurrently": "^2.0.0",
    "copy-webpack-plugin": "^3.0.0",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.0",
    "enzyme": "^2.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "flightplan": "^0.6.13",
    "html-webpack-plugin": "^2.15.0",
    "jscs": "^3.0.1",
    "jsdom": "^9.2.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.6.1",
    "mocha": "^2.4.5",
    "nightwatch": "^0.9.0",
    "nodemon": "^1.9.1",
    "react-addons-test-utils": "^15.0.0",
    "react-transform-hmr": "^1.0.4",
    "selenium-standalone": "^5.1.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.2",
    "stylus-loader": "^2.0.0",
    "template-string-loader": "0.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^2.5.0"
  },
  "dependencies": {
    "compression": "^1.6.1",
    "express": "^4.13.4",
    "express-robots": "^0.2.0",
    "helmet": "^2.1.0",
    "history": "^2.0.1",
    "inert": "^4.0.0",
    "munge": "^0.1.4",
    "react": "^15.0.0",
    "react-css-modules": "^3.7.6",
    "react-dom": "^15.0.0",
    "react-router": "^2.0.1",
    "serve-favicon": "^2.3.0"
  },
  "engines": {
    "node": ">=5.0.0"
  }
}
