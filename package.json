{
  "name": "daenen-react",
  "version": "1.0.0",
  "description": "Homepage for www.daenen4.de",
  "main": "server.js",
  "repository": "https://github.com/luhmann/daenen-react.git",
  "scripts": {
    "start-react": "BABEL_ENV=development webpack-dev-server",
    "watch-react": "webpack --watch",
    "build-react": "webpack",
    "watch-server": "webpack --config webpack.config.server.js --watch",
    "build-server": "webpack --config webpack.config.server.js",
    "nodemon-server": "nodemon ./dist/server.js",
    "start": "NODE_ENV=DEBUG concurrent --kill-others \"npm run watch-server\" \"npm run nodemon-server\" \"npm run watch-react\"",
    "build": "NODE_ENV=PROD npm run test && npm run build-react && npm run build-server",
    "test": "BABEL_ENV=test mocha --require ./test/setup.js --compilers js:babel-core/register --recursive",
    "test-debug": "BABEL_ENV=test mocha --debug-brk --require ./test/setup.js --compilers js:babel-core/register --recursive",
    "test-e2e": "nightwatch -e chrome",
    "postinstall": "selenium-standalone install",
    "lint": "jscs ."
  },
  "author": "Jan Florian Dietrich",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "chai": "^3.4.1",
    "clean-webpack-plugin": "^0.1.4",
    "concurrently": "^1.0.0",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.0",
    "css-modules-require-hook": "^2.1.0",
    "enzyme": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.8.4",
    "flightplan": "^0.6.0",
    "html-webpack-plugin": "^2.8.1",
    "jscs": "^2.7.0",
    "jsdom": "^8.0.2",
    "json-loader": "^0.5.3",
    "lodash": "^4.3.0",
    "mocha": "^2.3.4",
    "nightwatch": "^0.8.9",
    "nodemon": "^1.8.1",
    "react-addons-test-utils": "^0.14.3",
    "react-transform-hmr": "^1.0.1",
    "selenium-standalone": "^4.8.0",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.4.2",
    "template-string-loader": "0.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.12.1",
    "webpack-merge": "^0.7.1"
  },
  "dependencies": {
    "compression": "^1.6.0",
    "serve-favicon": "^2.3.0",
    "express": "^4.13.3",
    "express-robots": "^0.1.5",
    "history": "^2.0.0",
    "inert": "^3.2.0",
    "munge": "^0.1.4",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-router": "^2.0.0",
    "react-css-modules": "^3.6.1"
  },
  "engines": {
    "node": ">=5.0.0"
  }
}
